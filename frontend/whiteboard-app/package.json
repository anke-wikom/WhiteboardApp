        "ts-loader": "^9.4.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typescript": "*",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.3",
        "webpack-dev-server": "^4.15.0"
